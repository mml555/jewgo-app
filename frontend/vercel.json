{
  "version": 2,
  "name": "jewgo-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://jewgo.onrender.com"
  },
  "functions": {
    "app/**/*.ts": {
      "maxDuration": 30
    }
  }
} 