{
  "name": "jewgo-frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "health-check": "node scripts/health-check.js",
    "validate-env": "node scripts/validate-env.js",
    "check-env": "node scripts/check-environment.js",
    "deploy:check": "npm run validate-env && npm run health-check && echo 'âœ… Deployment validation complete'",
    "monitor:health": "node scripts/health-monitor.js",
    "monitor:setup": "node scripts/setup-monitoring.js",
    "monitor:test": "node scripts/test-monitoring.js",
    "seed:data": "node scripts/seed-test-data.js",
    "optimize:api": "node scripts/optimize-api.js",
    "analyze": "ANALYZE=true npm run build",
    "security:audit": "npm audit && npm audit fix",
    "security:fix": "npm audit fix --force",
    "bundle:analyze": "npm run build && npx @next/bundle-analyzer",
    "performance:check": "npm run build && npx lighthouse --output=json --output-path=./lighthouse-report.json",
    "monitor:start": "node scripts/health-monitor.js start",
    "monitor:check": "node scripts/health-monitor.js check",
    "monitor:report": "node scripts/health-monitor.js report",
    "monitor:rotate-logs": "node scripts/rotate-logs.js",
    "monitor:aggregate": "node scripts/aggregate-metrics.js",
    "monitor:cleanup": "node scripts/cleanup-logs.js"
  },
